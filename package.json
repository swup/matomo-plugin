{
  "name": "@swup/matomo-plugin",
  "amdName": "SwupMatomoPlugin",
  "version": "3.0.1",
  "description": "A swup plugin for integrating Matomo analytics",
  "type": "module",
  "source": "src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.modern.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "swup package:build",
    "dev": "swup package:dev",
    "lint": "swup package:lint",
    "format": "swup package:format",
    "prepublishOnly": "npm run build"
  },
  "author": {
    "name": "Jens Draser-Schieb",
    "email": "jens@berlinersueden.de",
    "url": "https://berlinersueden.de/"
  },
  "contributors": [
    {
      "name": "Georgy Marchuk",
      "email": "gmarcuk@gmail.com",
      "url": "https://gmrchk.com/"
    },
    {
      "name": "Rasso Hilber",
      "email": "mail@rassohilber.com",
      "url": "https://rassohilber.com/"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/swup/matomo-plugin.git"
  },
  "dependencies": {
    "@swup/plugin": "^4.0.0"
  },
  "devDependencies": {
    "@swup/cli": "^5.0.1"
  },
  "peerDependencies": {
    "swup": "^4.0.0"
  },
  "browserslist": [
    "extends @swup/browserslist-config"
  ],
  "prettier": "@swup/prettier-config"
}
